#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/razpeleg/repos/news-app-api/frontend/node_modules/metro/src/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules/metro/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules:/Users/razpeleg/repos/news-app-api/node_modules:/Users/razpeleg/repos/node_modules:/Users/razpeleg/node_modules:/Users/node_modules:/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/razpeleg/repos/news-app-api/frontend/node_modules/metro/src/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules/metro/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules:/Users/razpeleg/repos/news-app-api/node_modules:/Users/razpeleg/repos/node_modules:/Users/razpeleg/node_modules:/Users/node_modules:/node_modules:/Users/razpeleg/repos/news-app-api/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../metro/src/cli.js" "$@"
else
  exec node  "$basedir/../metro/src/cli.js" "$@"
fi
